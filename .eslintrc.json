{
    "rules": {
        "camelcase": 0,
        "curly": 1,
        "eqeqeq": 0,
        "indent": ["error", 4, {"SwitchCase": 1}],
        "max-len": 0,
        "max-nested-callbacks": 0,
        "no-alert": 0,
        "no-empty": 1,
        "no-mixed-requires": 0,
        "no-multiple-empty-lines": 0,
        "no-nested-ternary": 1,
        "no-obj-calls": 2,
        "no-unused-vars": 0,
        "no-use-before-define": 0,
        "no-warning-comments": 0,
        "one-var": 0,
        "padded-blocks": [2, "always"],
        "quotes": 0,
        "require-jsdoc": 0,
        "semi": 1,
        "space-before-blocks": [1, "always"],
        "space-before-function-paren": [2, "always"],
        "wrap-iife": [2, "outside"],
        "new-cap": 0
    },
    "globals": {},
    "extends": "google"
}
